<?xml version="1.0"?>
<object name="portal_controlpanel"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n"
   i18n:domain="collective.newsticker"
   purge="False">
 <configlet title="News Ticker Settings"
    action_id="NewsTickerSettings"
    appId="collective.newsticker"
    category="Products"
    condition_expr=""
    url_expr="string:${portal_url}/@@newsticker-settings"
    icon_expr="string:${portal_url}/++resource++collective.newsticker/newsticker_icon.png"
    visible="True"
    i18n:attributes="title">
  <permission>Manage portal</permission>
 </configlet>
</object>
